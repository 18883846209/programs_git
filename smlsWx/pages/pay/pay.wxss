	text-align: center;
}
.icon {
	width: 140rpx;
	height: 140rpx;
	margin: 0 auto;
	background: url('https://qiniu.image.cq-wnl.com/content/201804177a48aa44bbd54b3bb6eb9a649a0bd3fe.png') no-repeat;
	background-size: cover;
}
