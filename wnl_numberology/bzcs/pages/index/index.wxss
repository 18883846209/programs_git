}

.bzcsResultLabel {
  width: 70rpx;
  height: 224rpx;
  background: url('https://mobile.51wnl.com/numberology/bzcs/img/bzcs/cs-zdnr-bk@3x.png') no-repeat;
  background-size: 100% 100%;
  margin-right: 14rpx;
}

.bzcsResultLabelContent {
  font-family: zidingyi2 !important;
  font-size: 38rpx;
  line-height: 40rpx;
  text-align: center;
  color: #f16300;
  margin-top: 30rpx;
}

.bzcsResultLabel01 {
  margin-right: 0;
}

.yunshiDetailContent {
  margin: 30rpx 0;
  height: 414rpx;
  border-radius: 20rpx;
  background-color: #f5f0e9;
  overflow: hidden;
}

.yunshiDetailContent image {
  padding: 30rpx;
  box-sizing: border-box;
  height: 100%;
  width: 100%;
}

.feedBackNum {
  margin: 30rpx 0;
  font-size: 26rpx;
  line-height: 1;
  text-align: center;
  color: #999;
}

.feedBackNum::before {
  content: '已有 ';
}

.feedBackNum::after {
  content: ' 人解锁八字测算';
}

.feedback {
}

.cesuan_comments_content {
  clear: both;
  background: #f5f0e9;
  border-radius: 10px;
  height: 268px;
  padding: 0 15px;
  overflow: hidden;
  box-sizing: border-box;
  position: relative;
  border: 1px solid #f5f0e9;
}

.cesuan_comments_content::before {
  content: '';
  z-index: 1;
  width: 100%;
  height: 15px;
  top: 0;
  left: 0;
  position: absolute;
  background: #f5f0e9;
  overflow: hidden;
  border-radius: 10px;
}

.cesuan_comments_content::after {
  content: '';
  z-index: 1;
  width: 100%;
  height: 25px;
  bottom: 0;
  left: 0;
  position: absolute;
  border-radius: 10px;
  clear: both;
  background-image: linear-gradient(to bottom, rgba(245, 240, 233, 0) 0%, rgba(245, 240, 233, 1) 40%, rgba(245, 240, 233, 1) 100%);
}

.cesuan_comments {
  animation: scrollPage 10s ease-in-out farwards;
}

@keyframes scrollPage {
  0% {
    opacity: 1;
  }

  50% {
    margin-top: 500rpx;
  }

  100% {
    margin-top: 1000rpx;
    opacity: 0;
  }
}

.cesuan_comments .marqueeItem {
  padding-top: 15px;
  padding-bottom: 12px;
  border-bottom: 1px dotted rgba(153, 153, 153, 0.3);
}

.cesuan_comments .marqueeItem .name {
  font-size: 17px;
  font-weight: 500;
  line-height: 1;
  color: #333;
}

.cesuan_comments .marqueeItem .city {
  font-size: 13px;
  line-height: 1;
  color: #999;
  padding-left: 5px;
}

.cesuan_comments .marqueeItem .head {
  height: 17px;
  margin-bottom: 7px;
}

.cesuan_comments .marqueeItem .comment {
  font-size: 13px;
  line-height: 1.38;
  text-align: justify;
  color: #999;
}

.cesuan_comments .marqueeItem .stars {
  float: right;
  height: 15px;
  width: 85px;
}

.cesuan_comments .marqueeItem .stars .star {
  float: left;
  height: 15px;
  width: 15px;
  background-image: url('https://mobile.51wnl.com/numberology/lyys/img/cs-yhpl-xj-wxz@3x.png');
  background-size: cover;
  margin-left: 2px;
}

.cesuan_comments .marqueeItem .stars .starActive {
  float: left;
  height: 15px;
  width: 15px;
  background-image: url('https://mobile.51wnl.com/numberology/lyys/img/cs-yhpl-xj-xz@3x.png');
  background-size: cover;
}

