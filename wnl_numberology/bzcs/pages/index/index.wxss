.copyright {
  height: 22rpx;
  font-size: 11px;
  line-height: 1;
  text-align: center;
  color: #333;
  padding: 15px 0;
  border-top: 1px solid #eaeaea;
  margin-top: 30px;
  padding-bottom: 75px !important;
}

.confirm_btn_fixed {
  position: fixed;
  padding: 5px 15px;
  z-index: 10;
  height: 60px;
  bottom: 0;
  width: 100%;
  box-sizing: border-box;
  -webkit-backdrop-filter: blur(10px);
  backdrop-filter: blur(10px);
  background-color: rgba(252, 251, 249, 0.92);
  box-shadow: 0 -1px 0 0 rgba(0, 0, 0, 0.05);
  font-weight: 500;
  transition: bottom 0.6s ease-in-out;
}

.confirm_btn_fixed_link {
  height: 50px;
  line-height: 50px;
  border-radius: 10px;
  background-color: #ff9447;
  font-size: 20px;
  color: #fff;
  z-index: 10;
  text-align: center;
  opacity: 1;
  transition: bottom 0.6s ease-in-out;
}

.confirm_btn_fixed_hidden {
  bottom: -61px !important;
}

.toastContent {
  position: fixed;
  bottom: 40rpx;
  left: 0;
  right: 0;
  z-index: 9999;
  overflow: hidden;
  text-align: center;
  -webkit-transition: opacity 0.3s ease-out;
  -moz-transition: opacity 0.3s ease-out;
  -ms-transition: opacity 0.3s ease-out;
  -o-transition: opacity 0.3s ease-out;
  transition: opacity 0.3s ease-out;
}

.toast {
  display: inline-block;
  height: 60rpx;
  line-height: 60rpx;
  background-color: #000;
  color: #fff;
  text-align: center;
  border-radius: 20rpx;
  font-size: 28rpx;
  font-weight: 500;
  padding: 0 20rpx;
}

