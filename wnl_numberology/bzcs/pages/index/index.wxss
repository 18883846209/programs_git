/*indexContent  */

.indexContent {
  margin: 60rpx 30rpx 0 30rpx;
}

.box_line {
  display: -webkit-box;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-box-pack: justify;
  -webkit-box-align: stretch;
  -webkit-box-lines: single;
  display: flex;
  display: -webkit-flex;
  flex-direction: row;
  -webkit-flex-direction: row;
  justify-content: space-between;
  -webkit-justify-content: space-between;
  align-items: center;
  align-content: flex-start;
  -webkit-align-content: flex-start;
  flex-wrap: nowrap;
  -webkit-flex-wrap: nowrap;
}

.title_line {
  width: 100%;
  height: 1px;
  background-color: #ff9447;
  /* -webkit-box-flex: 1; *//* -webkit-flex: 1; */
  flex: 1;
}

.title_dot {
  width: 10rpx;
  height: 10rpx;
  border-radius: 100%;
  background-color: #ff9447;
}

.title_txt {
  margin: 0 20rpx;
  height: 34rpx;
  font-size: 17px;
  line-height: 34rpx;
  text-align: center;
  color: #ff9447;
  font-weight: 500;
}

.expertTeamSwiper {
  height: 388rpx;
}

swiper {
  height: 100%;
}

.dashiSwiper {
  margin-top: 30rpx;
}

.swiperBG {
  height: 358rpx;
  border-radius: 10px;
  background-color: #f5f0e9;
  top: 15px;
  position: absolute;
  box-sizing: border-box;
  background-image: url('http://mobile.51wnl.com/numberology/lyys/img/cs-bg@3x.png');
  background-position: bottom;
  background-repeat: repeat-x;
  background-size: 50px;
}

.swiperBGLeft {
  position: absolute;
  top: 0;
  width: 32.46%;
  width: calc(29.87vw);
  max-width: 120px;
  max-height: 194px;
  z-index: 1;
  height: 100%;
  height: calc(51.73vw);
}

.swiperBGLeft > image {
  width: 100%;
  height: 100%;
  position: absolute;
  bottom: 0;
  left: 0;
}

.swiperBGRight {
  margin-right: 30rpx;
  float: right;
  width: 61%;
}

.expertName {
  font-family: zidingyi !important;
  font-size: 48rpx;
  line-height: 1;
  text-align: left;
  color: #f16300;
  padding: 30rpx 0 20rpx 0;
}

.expertDesc {
  font-family: PingFangSC;
  font-size: 24rpx;
  line-height: 1.45;
  text-align: justify;
  color: #333;
  overflow: visible;
}

.cesuanResultContent, .cesuanYunshiContent, .cesuanFeedbackContent,
.cesuanIntroductionContent, .cesuanContactContent {
