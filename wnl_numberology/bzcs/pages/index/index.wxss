}

.cesuanResult {
  margin-top: 30rpx;
  height: 284rpx;
  border-radius: 20rpx;
  background-color: #f5f0e9;
  padding: 30rpx 20rpx 30rpx 20rpx;
  display: inline-flex;
  justify-content: center;
  flex-direction: row;
  align-items: center;
  width: 100%;
