}

.input-content {
  height: 88rpx;
  width: 100%;
  position: relative;
  z-index: 3;
}

.input-content01 {
  border-bottom: solid 2rpx #ff9447;
}

.input-content-label {
  font-family: PingFangSC;
  font-size: 34rpx;
  font-weight: 500;
  line-height: 1;
  text-align: center;
  color: #ff9447;
  float: left;
  padding: 26rpx 30rpx 28rpx 30rpx;
}

.input-content-value {
  float: left;
  font-family: PingFangSC;
  font-size: 34rpx;
  line-height: 1;
  text-align: left;
  color: #333;
  height: 100%;
  width: 490rpx;
}

.input-content-value text {
  width: 100%;
  height: 100%;
  float: left;
  padding: 28rpx 0 26rpx 0;
}

#spBirthDate {
  padding: 0;
  line-height: 88rpx;
}

.sexSelect {
  width: 90rpx;
  height: 34rpx;
  float: left;
  margin-top: 28rpx;
}

.sexIcon {
  width: 30rpx;
  height: 30rpx;
  float: left;
  margin-top: 4rpx;
}

.sexIcon image {
  width: 100%;
  height: 100%;
}

.sexTxt {
  float: left;
  margin-left: 20rpx;
}

.sexSelectFemale {
  margin-left: 130rpx;
}

.arrowContent {
  width: 9px;
  height: 100%;
  background-image: url(http://mobile.51wnl.com/numberology/bzcs/img/bzcs/ca-more-arrow-icon@3x.png);
  background-position: center;
  background-repeat: no-repeat;
  background-size: 9px 4px;
  /* float: right; *//* margin-right: 30rpx; */
  position: absolute;
  top: 0;
  right: 30rpx;
}

.confirmBtn {
  height: 50px;
  box-sizing: border-box;
  margin: 15px;
}
button {  
  height: 100%;
  line-height: 50px;
  border: none;
}
button[type="primary"] {
  color: #fff;
  background-color: #ff9447;
  border-radius: 14px;
}

button::after {
  border: 0 none;
}
.button-hover[type="primary"] {
  color: rgba(255, 255, 255, 0.6);
  background-color: #f26d0e;
  border-radius: 28rpx;
}

