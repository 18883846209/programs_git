/* pages/result/result.wxss */

::-webkit-scrollbar {
  width: 0;
  height: 0;
}

view {
  padding: 0;
  margin: 0;
}

.result {
  background-color: #fcfbf9;
}

.headerPanel {
  margin: 30rpx;
  border-radius: 20rpx;
  background-color: #f5f0e9;
  box-shadow: 0 2px 8px 0 rgba(0, 0, 0, 0.1);
}

.headerPanel::before {
  content: '十年运势卡';
  height: 34rpx;
  width: 150rpx;
  background: #f16300;
  background-image: linear-gradient(to right, rgba(0, 0, 0, 0) 10%, #f16300 10%);
  position: absolute;
  right: 30rpx;
  top: 60rpx;
  padding-top: 3rpx;
  font-size: 26rpx;
  line-height: 1;
  text-align: center;
  color: #fff;
  z-index: 2;
  box-sizing: border-box;
}

.headerPanel::after {
  content: "";
  height: 34rpx;
  width: 160rpx;
  z-index: 1;
  position: absolute;
  background: url(http://mobile.51wnl.com/numberology/bzcs/img/bzcs/jsq-snysk-bq@3x.png);
  background-size: contain;
  background-repeat: no-repeat;
  right: 30rpx;
  top: 60rpx;
}

.bzcs_name {
  padding: 15px 0 0 15px;
  font-size: 17px;
  font-weight: 500;
  line-height: 1;
  text-align: left;
  color: #333;
  display: inline-block;
}

.bzcs_sex {
  font-size: 13px;
  line-height: 1;
  text-align: left;
  color: #999;
  display: inline-block;
  margin-left: 10rpx;
}

.bzgl {
  margin-top: 30rpx;
}

.bzcs_gl, .bzcs_bz {
  padding: 6px 20px 0 17px;
  font-size: 13px;
  line-height: 1;
  text-align: left;
  color: #999;
  display: inline-block;
}

.bzcs_date_hr {
  height: 1px;
  opacity: 0.2;
  background-color: #f16300;
  margin: 10rpx 38rpx 0 138rpx;
}

.bzcs_year, .bzcs_bz_year {
  width: 130rpx;
}

.bzcs_month, .bzcs_bz_month {
  width: 84rpx;
  margin-left: 40rpx;
}

.bzcs_day, .bzcs_bz_day {
  width: 84rpx;
  margin-left: 50rpx;
}

.bzcs_time, .bzcs_bz_time {
  width: 84rpx;
  margin-left: 50rpx;
}
.bzcs_bzLabel {
  margin-top: 6rpx;
}
.bzcs_font {
  display: inline-block;
  font-size: 17px;
  line-height: 1;
  text-align: center;
  color: #333;
  padding-top: 12rpx;
  padding-bottom: 10rpx;
}

.bzcs_bzgl_hr, .bzcs_ysChart_hr {
  opacity: 0.3;
  border-bottom: 1px dotted #999;
  margin: 15px 15px 0 15px;
}

/*ysChart  */

.ysChartTitle {
  padding: 15px 0 0 15px;
  font-size: 34rpx;
  font-weight: 500;
  line-height: 1;
  text-align: left;
  color: #333;
}

.ysChartContent {
  margin: 30rpx 0 0 30rpx;
  /* height: 390rpx; */
  box-sizing: border-box;
}

.ysChartContent image {
  height: 100%;
  width: 100%;
}

.wxContentTitle {
  padding: 30rpx 0 0 30rpx;
  font-size: 34rpx;
  font-weight: 500;
  line-height: 1;
  text-align: left;
  color: #333;
}

.wxContentDetail {
  margin: 30rpx 30rpx 0 30rpx;
  border-radius: 200rpx;
  background-color: #fff;
  height: 21px;
}

.wxContentDetail>.left {
  width: 16%;
  font-size: 26rpx;
  font-weight: 500;
  line-height: 21px;
  color: #fff;
  text-align: left;
  padding-left: 4%;
}

.left {
  float: left;
}

.wxContentDetail :nth-child(1) {
  border-top-left-radius: 200rpx;
  border-bottom-left-radius: 200rpx;
  background: #f9b90e;
}

.wxContentDetail :nth-child(2) {
  background: #529671;
}

.wxContentDetail :nth-child(3) {
  background: #2ea9df;
}

.wxContentDetail :nth-child(4) {
  background: #f16300;
}

.wxContentDetail :nth-child(5) {
  border-top-right-radius: 100px;
  border-bottom-right-radius: 100px;
  background: #e1bb87;
}

.headerPanelBottom {
  margin: 32rpx 0 0 0;
  height: 100rpx;
  background: url('http://mobile.51wnl.com/numberology/bzcs/img/bzcs/cs-bg@3x.png');
  background-size: 50px 50px;
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
  text-align: center;
  position: relative;
}

.cardTxt {
  font-size: 17px;
  font-weight: 500;
  line-height: 104rpx;
  text-align: center;
  color: #333;
}

.cardTxt::after {
  content: '';
  width: 30rpx;
  height: 30rpx;
  background: url('http://mobile.51wnl.com/numberology/bzcs/img/bzcs/jsq-next-gq@3x.png');
  margin-left: 10rpx;
  position: absolute;
  cursor: pointer;
  top: 38rpx;
  background-size: cover;
}

/*dayunContent  */

.dayunContent {
  border-radius: 20rpx;
  background-color: #f5f0e9;
  margin: 30rpx;
  padding-bottom: 8rpx;
}

.dayunTitle {
  font-size: 22px;
  font-weight: 500;
  line-height: 1;
  text-align: left;
  color: #333;
  padding: 30rpx 0 10rpx 30rpx;
}

.dayunYearList {
  /* display: inline-flex; */
  justify-content: center;
  width: 50%;
  padding-bottom: 22rpx;
  padding-top: 22rpx;
  text-align: left;
  display: inline-block;
}

.dayunYearListBottomBorder {
  border-bottom: 1px dotted #999;
}

.dayunYearsDetail {
  margin: 2rpx 30rpx 14rpx 30rpx;
}

.dayunYearLabel {
  font-size: 17px;
  line-height: 1;
  text-align: left;
  color: #333;
  display: inline-block;
}

.dayunYearNum {
  font-size: 17px;
  line-height: 1;
  color: #999;
  margin-left: 10rpx;
  display: inline-block;
}

.xingqingContent {
  border-radius: 20rpx;
  background-color: #f5f0e9;
  margin: 30rpx;
}

.xingqingTitle {
  font-size: 22px;
  font-weight: 500;
  line-height: 1;
  text-align: left;
  color: #333;
  padding: 30rpx 0 0 30rpx;
}

.xingqingDetail {
  font-size: 17px;
  line-height: 1.59;
  text-align: justify;
  color: #333;
  padding: 20rpx 30rpx 30rpx 30rpx;
}

.xingshenContent {
  border-radius: 10px;
  background-color: #f5f0e9;
  margin: 30rpx;
  height: 348rpx;
}

.xingshenTitle {
  font-size: 22px;
  font-weight: 500;
  line-height: 1;
  text-align: left;
  color: #333;
  padding: 30rpx 0 0 30rpx;
}

.xingshenDetail {
  height: 274rpx;
  background-image: url('http://mobile.51wnl.com/numberology/bzcs/img/bzcs/cs-locking-img@3x.jpg');
  background-size: 100% 100%;
  cursor: pointer;
  border-radius: 20rpx;
  -webkit-tap-highlight-color: transparent; 
}

.xingshenLock {
  width: 56rpx;
  height: 80rpx;
  margin: 0 auto;
  padding-top: 40rpx;
}

.xingshenLock >image {
  width: 100%;
  height: 100%;
}

.xingshenNote {
  font-size: 17px;
  line-height: 1.29;
  text-align: center;
  color: #333;
  margin: 24rpx 30rpx 30rpx 30rpx;
}

.cesuanContactContent {
  height: 58rpx;
  border-radius: 30rpx;
  background-color: #f5f0e9;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  padding: 0 30rpx;
  box-sizing: border-box;
  margin: 30rpx 30rpx 0 30rpx;
  width: calc(100% - 60rpx);
}

.cesuanContactContentPaied {
  margin: 30rpx 30rpx 0 30rpx;
}

.contactTxt {
  font-family: SFProText;
  font-size: 13px;
  line-height: 1;
  color: #999;
}

.contactPhone {
  margin-left: 54rpx;
}

.copyright {
  height: 11px;
  font-size: 11px;
  line-height: 1;
  text-align: center;
  color: #333;
  padding: 15px 0;
  border-top: 1px solid #eaeaea;
  margin-top: 30px;
  padding-bottom: 75px;
}
.copyrightPaid {
  padding-bottom: 40rpx !important;
}
.confirm_btn_fixed {
  position: fixed;
  padding: 10rpx 30rpx;
  z-index: 10;
  height: 60px;
  bottom: 0;
  width: 100%;
  box-sizing: border-box;
  -webkit-backdrop-filter: blur(10px);
  backdrop-filter: blur(10px);
  background-color: rgba(252, 251, 249, 0.92);
  box-shadow: 0 -1px 0 0 rgba(0, 0, 0, 0.05);
  font-weight: 500;
  transition: bottom 0.6s ease-in-out;
}
button[type="primary"] {
  color: #fff;
  background-color: #ff9447;
  border-radius: 28rpx;
}

.button-hover[type="primary"] {
  color: rgba(255, 255, 255, 0.6);
  background-color: #f26d0e;
  border-radius: 28rpx;
}

.confirm_btn_fixed_link {
  height: 50px;
  line-height: 50px;
  border-radius: 20rpx;
  background-color: #ff9447;
  font-size: 20px;
  color: #fff;
  z-index: 10;
  text-align: center;
  opacity: 1;
  transition: bottom 0.6s ease-in-out;
}

/*resultPaidContent  */

.wxContentPieChart {
  height: 236rpx;
  margin: 32rpx 0 30rpx 30rpx;
}

.wxContentLeft {
  width: 240rpx;
  height: 100%;
  /* background-color: #e0bb87;
  border-radius: 240rpx; */
  display: inline-block;
  position: relative;
}

.canvasWXPie {
  position: absolute;
  top: -40rpx;
  left: -40rpx;
  overflow: hidden;
}

.ysChartLine {
  height: 340rpx;
  width: 640rpx;
}

.wxContentRight {
  margin-top: 26rpx;
  margin-left: 40rpx;
  width: 370rpx;
  display: inline-block;
  vertical-align: top;
}

.wxDetail {
  display: inline-block;
  width: 185rpx;
  margin-bottom: 8rpx;
}

.wxIcon {
  height: 26rpx;
  width: 26rpx;
  border-radius: 26rpx;
  display: inline-block;
}

.wxDetail:nth-child(1) .wxIcon {
  background-color: #f9b90e;
}

.wxDetail:nth-child(2) .wxIcon {
  background-color: #529671;
}

.wxDetail:nth-child(3) .wxIcon {
  background-color: #2ea9df;
}

.wxDetail:nth-child(4) .wxIcon {
  background-color: #f16300;
}

.wxDetail:nth-child(5) .wxIcon {
  background-color: #e0bb87;
}

.wxValue {
  font-size: 17px;
  line-height: 1;
  text-align: left;
  color: #333;
  margin-left: 10rpx;
  display: inline-block;
}

.wuxingxijiContent {
  border-radius: 20rpx;
  background-color: #f5f0e9;
  padding-bottom: 24rpx;
  margin: 30rpx;
}

.wuxingxijiTitle, .dayunYearDetailTitle, .dayunYearTenTitle {
  font-size: 22px;
  font-weight: 500;
  line-height: 1;
  text-align: left;
  color: #333;
  padding: 30rpx 0 0 30rpx;
}

.wuxingxijiTitleS {
  margin: 30rpx 0 0 30rpx;
  font-size: 17px;
  font-weight: 500;
  line-height: 1;
  text-align: left;
  color: #f16300;
}

.wuxingxijiContentHr {
  margin: 24rpx 15px 0 20rpx;
  opacity: 0.3;
  border-bottom: 1px dotted #999;
}

.wuxingxijiDetail {
  font-size: 17px;
  line-height: 1.59;
  text-align: justify;
  color: #333;
  padding: 10rpx 30rpx 0 30rpx;
}

.liuNianContent {
  margin: 30rpx;
}

/*liuNian  */

.dayunYearDetailContent {
  border-radius: 10px;
  background-color: #f5f0e9;
  padding-bottom: 12px;
  margin-top: 15px;
}

.wuxingxijiTitle, .dayunYearDetailTitle, .dayunYearTenTitle {
  font-size: 22px;
  font-weight: 500;
  line-height: 1;
  text-align: left;
  color: #333;
  padding: 15px 0 0 15px;
}

.dayunYearDetailTitle, .wuxingxijiTitle, .dayunYearDetailTitle {
  display: inline-block;
}

.dayunYearDetailScore {
  font-size: 20px;
  font-weight: 500;
  line-height: 1;
  text-align: left;
  color: #f16300;
  margin-left: 5px;
  display: inline-block;
}

.dayunYearDetailjpTtitle, .dayunYearDetailjsTtitle, .dayunYearDetailxsTtitle,
.dayunYearDetailllssTtitle {
  font-size: 17px;
  font-weight: 500;
  line-height: 1;
  text-align: left;
  color: #f16300;
  padding: 16px 0 0 15px;
}

.dayunYearDetailjpContent, .dayunYearDetailjsContent, .dayunYearDetailxsContent,
.dayunYearDetailllssContent {
  font-size: 17px;
  line-height: 1.59;
  text-align: justify;
  color: #333;
  padding: 5px 15px 0 15px;
}

.dayunYearDetailHr {
  opacity: 0.3;
  border-bottom: 1px dotted #999;
  margin: 12px 15px 0 15px;
}

.dayunYearTenContent {
  border-radius: 10px;
  background-color: #f5f0e9;
  padding-bottom: 12px;
  margin-top: 15px;
  box-shadow: 0 2px 8px 0 rgba(0, 0, 0, 0.1);
}

.wuxingxijiTitle, .dayunYearDetailTitle, .dayunYearTenTitle {
  font-size: 22px;
  font-weight: 500;
  line-height: 1;
  text-align: left;
  color: #333;
  padding: 15px 0 0 15px;
}

.dayunYearTenScoreTitle, .dayunYearTenQLTitle, .dayunYearTenSSTitle,
.dayunYearTenCHTitle {
  padding: 15px 0 0 15px;
  font-size: 17px;
  font-weight: 500;
  line-height: 1;
  text-align: left;
  color: #f16300;
  display: inline-block;
}

.dayunYearTenScoreNum {
  font-size: 17px;
  font-weight: 500;
  line-height: 1;
  text-align: left;
  color: #f16300;
  margin-left: 1px;
  display: inline-block;
}

.dayunYearTenScoreContent, .dayunYearTenQLContent, .dayunYearTenSSContent,
.dayunYearTenCHContent {
  font-size: 17px;
  line-height: 1.59;
  text-align: justify;
  color: #333;
  padding: 5px 15px 0 15px;
}
@media screen and (min-width: 310px) and (max-width: 350px) {
  .dayunYearLabel,.dayunYearNum{
    font-size: 15px;
  }
  .confirm_btn_fixed {
    height: 55px;
  }
  .confirm_btn_fixed_link {
    height: 45px;
    line-height: 45px;
  }
  .contactTxt {
    font-size: 11px;
  }
  .copyright {
    padding-bottom: 70px;
  }
  .bzcs_font {
    font-size: 15px;
  }
  .xingshenNote {
    font-size: 15px;
  }
}

