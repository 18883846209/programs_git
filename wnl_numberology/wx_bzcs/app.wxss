/*  button  */

.historyBtnScroll {
  bottom: 65px !important;
}

.historyBtn {
  font-size: 32rpx;
  line-height: 76rpx;
  position: fixed;
  box-sizing: border-box;
  text-align: right;
  z-index: 1000;
  right: 30rpx;
  width: 224rpx;
  bottom: 20rpx;
  border-radius: 40rpx;
  height: 76rpx;
  /* -webkit-backdrop-filter: blur(5px);
  backdrop-filter: blur(5px); */
  background-color: rgba(96, 96, 96, 0.95);
  box-shadow: 0 1px 2px 0 rgba(0, 0, 0, 0.1);
  color: #fff;
  text-decoration: none;
  /* padding: 12px; */
  padding-right: 30rpx;
  transition: bottom 0.6s ease-in-out;
}

.historyBtn::before {
  content: "";
  position: absolute;
  z-index: 1000;
  left: 30rpx;
  top: 24rpx;
  width: 24rpx;
  height: 30rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: cover;
}


